<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.npc.lottery.sysmge.entity">
	<class
		name="ManagerStaff"
		table="TB_FRAME_MANAGER_STAFF"
	>
		<id name="ID" type="java.lang.Long">
            <column name="ID" precision="12" scale="0" />
            <generator class="sequence">
				<param name="sequence">SEQ_TB_FRAME_MANAGER_STAFF</param>
			</generator>
        </id>
		
		<property
			name="account"
			column="ACCOUNT"
			type="java.lang.String"
			not-null="true"
			length="100"
		/>
		<property
			name="flag"
			column="FLAG"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="userType"
			column="USER_TYPE"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="userExtInfoId"
			column="USER_EXT_INFO_ID"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="userPwd"
			column="USER_PWD"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="chsName"
			column="CHS_NAME"
			type="java.lang.String"
			not-null="false"
			length="100"
		/>
		<property
			name="engName"
			column="ENG_NAME"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="emailAddr"
			column="EMAIL_ADDR"
			type="java.lang.String"
			not-null="false"
			length="100"
		/>
		<property
			name="officePhone"
			column="OFFICE_PHONE"
			type="java.lang.String"
			not-null="false"
			length="100"
		/>
		<property
			name="mobilePhone"
			column="MOBILE_PHONE"
			type="java.lang.String"
			not-null="false"
			length="100"
		/>
		<property
			name="fax"
			column="FAX"
			type="java.lang.String"
			not-null="false"
			length="100"
		/>
		<property
			name="createDate"
			column="CREATE_DATE"
			type="timestamp"
			not-null="false"
			length="7"
		/>
		<property
			name="updateDate"
			column="UPDATE_DATE"
			type="timestamp"
			not-null="false"
			length="7"
		/>
		<property
			name="loginDate"
			column="LOGIN_DATE"
			type="timestamp"
			not-null="false"
			length="7"
		/>
		<property
			name="loginIp"
			column="LOGIN_IP"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="comments"
			column="COMMENTS"
			type="java.lang.String"
			not-null="false"
			length="400"
		/>
		<property
			name="parentStaffQry"
			column="PARENT_STAFF_Qry"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="parentStaffTypeQry"
			column="PARENT_STAFF_TYPE_Qry"
			type="string"
			length="1"
		/> 
		<property
			name="passwordUpdateDate"
			column="PASSWORD_UPDATE_DATE"
			type="date"
		/> 
		<property
			name="passwordResetFlag"
			column="PASSWORD_RESET_FLAG"
			type="string"
		/> 
		<one-to-one name="chiefStaffExt"/>
		  
	</class>	
</hibernate-mapping>
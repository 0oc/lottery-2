<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.npc.lottery.sysmge.entity">
	<class
		name="Staff"
		table="TB_FRAME_STAFF"
	>
		<id name="ID" type="java.lang.Long">
            <column name="ID" precision="12" scale="0" />
            <generator class="sequence">
				<param name="sequence">SEQ_TB_FRAME_STAFF</param>
			</generator>
        </id>
		<property
			name="orgID"
			column="ORG_ID"
			type="long"
			not-null="false"
			length="10"
		/>
		<property
			name="account"
			column="ACCOUNT"
			type="string"
			not-null="false"
			length="80"
		/>
		<property
			name="flag"
			column="FLAG"
			type="string"
			not-null="false"
			length="1"
			insert="false"
		/>
		<property
			name="userPwd"
			column="USER_PWD"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="chsName"
			column="CHS_NAME"
			type="string"
			not-null="false"
			length="80"
		/>
		<property
			name="engName"
			column="ENG_NAME"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="emailAddr"
			column="EMAIL_ADDR"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="officePhone"
			column="OFFICE_PHONE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="mobilePhone"
			column="MOBILE_PHONE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="fax"
			column="FAX"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="createDate"
			column="CREATE_DATE"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="updateDate"
			column="UPDATE_DATE"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="loginDate"
			column="LOGIN_DATE"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="loginIp"
			column="LOGIN_IP"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="comments"
			column="COMMENTS"
			type="string"
			not-null="false"
			length="255"
		/>
	</class>	
</hibernate-mapping>
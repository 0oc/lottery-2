<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.npc.lottery.user.entity">
	<class
		name="UserCommissionDefault"
		table="TB_USER_COMMISSION_DEFAULT"
	>
		<id
			name="ID"
			type="java.lang.Long"
			column="ID"
		>
			 <generator class="sequence">
				<param name="sequence">SEQ_TB_USER_COMMISSION_DEFAULT</param>
			</generator>
		</id>

		<property
			name="ID"
			column="ID"
			type="java.lang.Long"
			not-null="true"
			length="10"
			insert="false" update="false"
		/>
		<property
			name="userId"
			column="USER_ID"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="userType"
			column="USER_TYPE"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="playType"
			column="PLAY_TYPE"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="playFinalType"
			column="PLAY_FINAL_TYPE"
			type="string"
			not-null="true"
			length="30"
		/>
		<property
			name="commissionA"
			column="COMMISSION_A"
			type="double"
			not-null="false"
			length="22"
		/>
		<property
			name="commissionB"
			column="COMMISSION_B"
			type="double"
			not-null="false"
			length="22"
		/>
		<property
			name="commissionC"
			column="COMMISSION_C"
			type="double"
			not-null="false"
			length="22"
		/>
		<property
			name="bettingQuotas"
			column="BETTING_QUOTAS"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="itemQuotas"
			column="ITEM_QUOTAS"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="createUser"
			column="CREATE_USER"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
		/>
		<property
			name="modifyUser"
			column="MODIFY_USER"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="modifyTime"
			column="MODIFY_TIME"
			type="timestamp"
			not-null="false"
			length="7"
		/>
        <property
			name="totalQuatas"
			column="TOTAL_QUOTAS"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="lowestQuatas"
			column="LOWEST_QUOTAS"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="winQuatas"
			column="WIN_QUOTAS"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="loseQuatas"
			column="LOSE_QUOTAS"
			type="integer"
			not-null="false"
			length="22"
		/>

	</class>	
</hibernate-mapping>